{
  "files.associations": {
      "settings.json": "jsonc",
      "*.tsx": "typescriptreact"
  },
  "workbench.iconTheme": "vscode-icons",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "files.eol": "\n",
  "editor.tabCompletion": "on",
  "diffEditor.codeLens": true,
  "prettier.printWidth": 78,
  "editor.rulers": [
      80,
      120
  ],
  "workbench.colorCustomizations": {
      "editorRuler.foreground": "#ff4081",
      "tab.activeBorderTop": "#ff6f00",
      "tab.unfocusedActiveBorder": "#000000",
      "editor.lineHighlightBackground": "#0d0d0dca",
      "statusBar.background": "#1a1f2f",
      "statusBar.foreground": "#ffff037a",
      "statusBar.border": "#f2ff0333",
      "statusBarItem.hoverBackground": "#0d2618",
      "statusBarItem.hoverForeground": "#ffbc04",
      "editorCursor.foreground": "#ff6f00",
      "scrollbarSlider.background": "#ff6f0022",
      "scrollbarSlider.hoverBackground": "#53240171",
      "scrollbarSlider.activeBackground": "#b34f0274"
  },
  "files.watcherExclude": {
      "**/.git/objects/**": true,
      "**/.git/subtree-cache/**": true,
      "**/node_modules/*/**": true,
      "**/.hg/store/**": true,
      "**/.next/objects/**": true
  },
  "editor.multiCursorLimit": 720,
  "prettier.bracketSpacing": true,
  "editor.tokenColorCustomizations": {
      "textMateRules": [
          {
              "scope": [
                  "source.js punctuation.terminator",
                  "source.jsx punctuation.terminator",
                  "source.ts punctuation.terminator",
                  "source.tsx punctuation.terminator"
              ],
              "settings": {}
          }
      ]
  },
  "files.exclude": {
      "**/.git": false
  },
  "git.decorations.enabled": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": false,
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
  },
  "emmet.triggerExpansionOnTab": true,
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.includeLanguages": {
      "typescript": "tyescriptreact",
      "javascript": "javascriptreact",
      "typescript-react": "typescriptreact",
      "javascript-react": "javascriptreact",
      "razor": "html",
      "plaintext": "pug"
  },
  "prettier.configPath": ".prettierrc.json",
  "prettier.requireConfig": true,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "git.enableSmartCommit": true,
  "tailwindCSS.experimental.configFile": null,
  "tailwindCSS.experimental.classRegex": [
      [
          "cva\\(([^)]*)\\)",
          "[\"'`]([^\"'`]*).*?[\"'`]"
      ]
  ],
  "editor.wordWrap": "wordWrapColumn",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.inlineSuggest.enabled": true,
  "explorer.confirmDragAndDrop": false,
  "editor.fontSize": 18.7,
  "editor.lineHeight": 1.53,
  "prettier.singleAttributePerLine": true
}